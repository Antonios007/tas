<?xml version="1.0"?>
<configuration>
    <startup> 
        
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/></startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="configConsoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
        <add name="configTextListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="DEBUG.log"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
